# Analytics & Error Tracking
VITE_SENTRY_DSN=your_sentry_dsn_here
VITE_APP_VERSION=1.0.0

# Email Service (escolha um)
# Resend (recomendado)
VITE_EMAIL_PROVIDER=resend
VITE_EMAIL_API_KEY=your_resend_api_key

# SendGrid
# VITE_EMAIL_PROVIDER=sendgrid
# VITE_EMAIL_API_KEY=your_sendgrid_api_key

# Backend customizado
# VITE_BACKEND_URL=https://api.seu-dominio.com

# Email admin para notificações de contato
VITE_ADMIN_EMAIL=admin@tatianatorres.com

# Google Analytics (opcional)
VITE_GA_ID=your_google_analytics_id

# AI Chatbot (OpenAI)
VITE_OPENAI_API_KEY=your_openai_api_key
VITE_OPENAI_MODEL=gpt-3.5-turbo

# Feature Flags
VITE_ENABLE_ANALYTICS=true
VITE_ENABLE_EMAIL=true
VITE_ENABLE_BOOKING=false

# API Keys (se usar)
VITE_GOOGLE_MAPS_API_KEY=your_google_maps_key
